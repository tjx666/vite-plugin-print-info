{
    "compilerOptions": {
        "target": "es2022",
        "module": "esnext",
        "strict": true,
        "esModuleInterop": true,
        "moduleResolution": "node",
        "skipLibCheck": true,
        "noUnusedLocals": false,
        "resolveJsonModule": true,
        "types": ["node"]
    },
    "include": ["*.ts"]
}
